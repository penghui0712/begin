.contain-view{
  width: 1200px;
  height: 640px;
  margin: 10px auto;
  font-size: 0;
  border: 1px solid #eaeaea;
  overflow: hidden;
  .left-menu{
    width: 80px;
    height: 100%;
    background: #353E45;
    display: inline-block;
    vertical-align: middle;
    .user-icon-img{
      width: 55px;
      height: 55px;
      display: inline-block;
      margin: 26px auto auto 12.5px;
    }
    .menu-list{
      margin-top: 84px;
      img{
        display: block;
        margin: 18px auto;
      }
      .menu-btn{
        cursor: pointer;
        img{
          width: 28px;
          height: 28px;
        }
      }
      .menu-line{
        width: 60px;
        margin: 18px auto auto auto;
        height: 1px;
        background: #3D474F;
      }
      .right-arrow{
        position: absolute;
        width: 0;
        height: 0;
        margin-top: -40px;
        margin-left: 80px;
        z-index: 999;
        border-top: 10px solid transparent;
        border-left: 15px solid #353E45;
        border-bottom: 10px solid transparent;
        display: none;
      }
      .active{
        .right-arrow{
          display: inline-block;
        }
      }
    }
    .setting-btn{
      display: block;
      cursor: pointer;
      width: 24px;
      height: 24px;
      margin: 230px auto auto 28px;
      .setting-btn-img{
        width: 24px;
        height: 24px;
      }
    }
    .custom-menu{
      left:150px;
      margin-top:-132px;
      background: #F0F0F0;
      border: 1px solid #DEDEDE;
      box-shadow: 0px 2px 4px 0px rgba(0,0,0,0.30);
      border-radius: 10px;
    }
  }
